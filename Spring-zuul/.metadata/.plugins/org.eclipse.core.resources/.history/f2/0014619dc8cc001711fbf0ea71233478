package com.example.jersy.jersydemo;

import org.springframework.boot.SpringApplication;
import org.springframework.boot.autoconfigure.SpringBootApplication;
import org.springframework.boot.builder.SpringApplicationBuilder;
import org.springframework.boot.web.support.SpringBootServletInitializer;

@SpringBootApplication
public class JersydemoApplication extends SpringBootServletInitializer{

	public static void main(String[] args) {
		SpringApplication.run(JersydemoApplication.class, args);
		
		 new JersydemoApplication().configure(new SpringApplicationBuilder(JersydemoApplication.class)).run(args);
	}
}
